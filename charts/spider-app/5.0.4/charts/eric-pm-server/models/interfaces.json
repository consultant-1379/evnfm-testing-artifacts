        "provides": {
          "netIf": [
            {
              "id": "IF.ADP.PM.PROM.PROMQL.V1",
              "description": "An interface that allows interested entities to query and access collected metric data",
              "specUri": "prometheus.io/docs/prometheus/latest/querying/api/",
              "specVer": "1.0.0",
              "type": "HttpJson",
              "setup": "direct",
              "maturity": "stable",
              "scope": "clusterInternal"
            }
          ]
        }
      },
      {
        "id": "IMPL.ADP.PM.IF.PROM.SCRAPING",
